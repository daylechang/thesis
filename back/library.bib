journal = {Applied Thermal Engineering},
keywords = {Boil-off gas (BOG) compressor,Computational fluid dynamics (CFD),Liquefied natural gas (LNG) system,Suction heating,Temperature distribution},
month = {sep},
pages = {1147--1157},
