@inproceedings{Clemens2009,
abstract = {Shock wave / boundary layers interactions occur in wide-range of supersonic internal and external flows, and often these interactions are associated with severe turbulent boundary layer separation. The shock-induced separation is associated with unsteady motions that are characterized by a wide range of frequencies, which include those characteristic of the upstream boundary layer as well as motions whose frequencies are about two orders of magnitude lower. Despite significant work over the past few decades, the source of the low-frequency motions is still an open question and is currently a topic of intense debate. Owing to a flurry of activity over the past decade on this single topic, both by computational and experimental researchers, we are close to developing a comprehensive understanding of the mechanisms that lead to low-frequency unsteadiness. Recent investigations on the source of low-frequency unsteadiness have provided what seems to be contradictory evidence since some indicate that the driving mechanism resides in the upstream boundary layer, whereas others suggest the dominant mechanism is an intrinsic instability of the separated flow. In this paper, based on a review of the evidence, we argue for a view where both processes are present in all shock-induced turbulent separated flows, but the relative importance of each mechanism depends on the severity of separation. In cases where the separation bubble is small, such as occurs for a small flow deflection or at high Reynolds number, then momentum fluctuations near the wall cause a direct movement of the separation line and can lead to collapse of the separation bubble. Upstream fluctuations may also pass through the separation shock, grow in the separated shear layer, and cause flapping of the reattachment point. In cases where the separation bubble is large, then the bubble pulsates owing to a global instability, and upstream turbulence serves mainly as a source of broadband frequencies that seed the large-scale instability of the separated flow. For strong separation, the separation line (and separation shock foot) is influenced on a small-scale by boundary layer momentum fluctuations, but large-scale motions are driven by reattachment line fluctuations. American Institute of Aeronautics and Astronautics 2 Nomenclature C f = skin friction coefficient f = frequency f c = zero-crossing frequency for separation shock foot h = separation bubble height L = separated flow length L i = intermittent region length M = Mach number P = pressure q = dynamic pressure R m = reattachment location Re T = Reynolds number based on upstream momentum thickness SK m = separation shock location at edge of boundary layer S m = instantaneous separation location St = Strouhal number t = time T = time period T m = mean time period u = instantaneous velocity U l = line-averaged upstream velocity u W = friction velocity U  = freestream velocity x = streamwise coordinate x sep = separation line surrogate y = wall-normal coordinate z = in one figure, wall-normal coordinate Į = flow turning angle along separation bubble GG 0 = upstream undisturbed boundary layer thickness J = ratio of specific heats T = upstream boundary layer momentum thickness Subscript = free stream conditions},
author = {Clemens, Noel and Narayanaswamy, Venkataswaran},
booktitle = {39th AIAA Fluid Dynamics Conference},
doi = {10.2514/6.2009-3710},
file = {:C$\backslash$:/Users/Dayle/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Clemens, Narayanaswamy - 2009 - ShockTurbulent Boundary Layer Interactions Review of Recent Work on Sources of Unsteadiness (Invited).pdf:pdf},
isbn = {978-1-60086-971-6},
pages = {2009--3710},
title = {{Shock/Turbulent Boundary Layer Interactions: Review of Recent Work on Sources of Unsteadiness (Invited)}},
url = {https://arc-aiaa-org.wrs.idm.oclc.org/doi/pdf/10.2514/6.2009-3710},
year = {2009}
}
@inproceedings{Eichorn2013a,
abstract = {Phase II data results of the Fundamental Inlet Bleed Experiments study at NASA Glenn Research Center are presented which include flow coefficient behavior for twenty-one bleed hole configurations. The bleed configurations are all round holes with hole diameters ranging from 0.795 to 6.35 mm, hole inclination angles from 20 to 90 degrees, and thickness- to-diameter ratios from 0.25 to 2.0. All configurations were tested at a unit Reynolds number of 2.46x107 /m and at discrete local Mach numbers of 1.33, 1.62, 1.98, 2.46, and 2.92. Interactions between the design parameters of hole diameter, hole inclination angle, and thickness-to-diameter as well as the interactions between the flow parameters of pressure ratio and Mach number upon the flow coefficient are examined, and a preliminary statistical model is proposed. An existing correlation is also examined with respect to this data.},
