title = {{Turbulence modeling- An overview}},
url = {https://arc-aiaa-org.wrs.idm.oclc.org/doi/pdf/10.2514/6.2001-724},
year = {2001}
}
@article{DeFeo,
abstract = {In this paper RANS modeling is used to simulate the supersonic base flowfield of an axisymmetric body. The CFD package FLUENT was used to obtain a grid converged solution of the essentially axisymmetric problem. Simple analytical cases were used to verify the numerical solver and well documented experimental results validated the numerical solution. Computational base pressure, velocity and turbulent kinetic energy estimates in the near-wake region are presented using the Spalart-Almaras, k-$\omega$, and Reynold Stress Models. The present results indicate that a correct boundary layer growth prediction, a correct description of the benchmark experimental setup coupled with an iteratively converged RSM approach can lead to base pressure results on par with more computationally demanding LES simulations. Nomenclature C Pbase = base pressure coefficient k = turbulent kinetic energy M = mach number P 1 = approach pressure P base = base pressure u 1 = approach velocity u $\tau$ = friction velocity u + = normalized velocity $\mu$ eff = sum of turbulent and laminar viscosities $\mu$ T = turbulent viscosity $\nu$ = kinematic viscosity},
