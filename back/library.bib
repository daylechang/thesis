title = {{A Source-Term Based Boundary Layer Bleed/Effusion Model for Passive Shock Control}},
url = {https://ntrs.nasa.gov/search.jsp?R=20110011133{\&}hterms=effect+bleed+hole+inclinination{\&}qs=N{\%}3D0{\%}26Ntk{\%}3DAll{\%}26Ntx{\%}3Dmode{\%}2Bmatchallany{\%}26Ntt{\%}3Deffect{\%}2Bof{\%}2Bbleed{\%}2Bhole{\%}2Binclinination},
year = {2011}
